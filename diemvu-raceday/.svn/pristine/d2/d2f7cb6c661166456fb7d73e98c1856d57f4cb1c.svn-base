
package model;

/**
 * 
 * @author diemvu
 * @version fall 2018
 */
public class Participant {
    /**
     * 
     */
    private static String C = ":";
    /**
     * 
     */
    private String myRacerID;
    /**
     * 
     */
    private String myRacerName;
    /**
     * 
     */
    private String myRacerDistance;
    
    /**
     * 
     * @param theRacerID racer
     * @param theRacerName racer
     * @param theRacerDistance racer
     */

    public Participant(final String theRacerID, final String theRacerName,
                       final String theRacerDistance) {
        myRacerID = theRacerID;
        myRacerName = theRacerName;
        myRacerDistance = theRacerDistance;
    }

    /**
     * @return Message
     */
    public String toString() {
        final String s = myRacerID + C + myRacerName + C + myRacerDistance;
        return s;
    }

    /**
     * @return the myRacerID
     */
    public String getMyRacerID() {
        return myRacerID;
    }

    /**
     * @param theRacerID the myRacerID to set
     */
    public void setMyRacerID(final String theRacerID) {
        this.myRacerID = theRacerID;
    }

    /**
     * @return the myRacerName
     */
    public String getMyRacerName() {
        return myRacerName;
    }

    /**
     * @param theRacerName the myRacerName to set
     */
    public void setMyRacerName(final String theRacerName) {
        this.myRacerName = theRacerName;
    }

    /**
     * @return the myRacerDistance
     */
    public String getMyRacerDistance() {
        return myRacerDistance;
    }

    /**
     * @param theRacerDistance the myRacerDistance to set
     */
    public void setMyRacerDistance(final String theRacerDistance) {
        this.myRacerDistance = theRacerDistance;
    }
}
